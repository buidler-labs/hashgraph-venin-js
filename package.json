{
  "publishConfig": {
    "access": "public"
  },
  "name": "@buidlerlabs/hedera-strato-js",
  "description": "The Hedera unoficial, high-level 'stratospheric' SDK for JS consumption",
  "scripts": {
    "build:rollup": "rollup -c",
    "build:ts": "tsc",
    "docs": "typedoc && node ./script/post-docs.cjs",
    "publish:npm": "rm -fr ./build && npm run build:ts && node ./script/prepare-publish.cjs && npm publish ./build --access public",
    "test": "npm run test:node && npm run test:web",
    "test:node": "jest --env=./test/jte.node.ts",
    "test:web": "jest --env=./test/jte.web.ts --setupFilesAfterEnv core-js"
  },
  "keywords": [
    "Hedera",
    "Hashgraph",
    "Smart Contracts"
  ],
  "author": "BuiDler Labs Developers <devs@buidlerlabs.com>",
  "license": "MIT",
  "dependencies": {
    "@ethersproject/abi": "^5.5.0",
    "@hashgraph/sdk": "^2.6.0",
    "axios": "^0.25.0",
    "dotenv": "^10.0.0",
    "solc": "0.8.9",
    "traverse": "^0.6.6",
    "tslib": "^2.3.1",
    "winston": "^3.4.0"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "^4.4.2",
    "@rollup/plugin-commonjs": "^22.0.0-4",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "core-js": "^3.20.3",
    "jest": "^27.4.5",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.1.2",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/buidler-labs/hedera-strato-js"
  },
  "types": "./index.d.ts",
  "version": "0.6.10"
}
