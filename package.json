{
  "author": "BuiDler Labs Developers <devs@buidlerlabs.com>",
  "description": "The Hedera unoficial, high-level 'stratospheric' SDK for JS consumption",
  "engines": {
    "node": ">=14.8"
  },
  "exports": {
    ".": {
      "node": "./lib.cjs/index.js",
      "default": "./lib.esm/index.js"
    },
    "./rollup-plugin": {
      "node": "./lib.cjs/bundler/rollup/plugin.js",
      "default": "./lib.esm/bundler/rollup/plugin.js"
    }
  },
  "files": [
    "lib.cjs",
    "lib.esm",
    "types"
  ],
  "main": "./lib.cjs/index.js",
  "module": "./lib.esm/index.js",
  "name": "@buidlerlabs/hedera-strato-js",
  "scripts": {
    "build:4-publish": "run-s build:cjs build:esm build:declarations",
    "build:cjs": "babel --env-name cjs lib --extensions .ts --out-dir lib.cjs --source-maps",
    "build:esm": "babel --env-name esm lib --extensions .ts --out-dir lib.esm --source-maps",
    "build:bundle-docs": "run-s build:bundle-docs-hedera-sdk build:bundle-docs-hashconnect build:bundle-docs-strato build:bundle-docs-app",
    "build:bundle-docs-app": "rollup -c ./lib.docs/src/app/rollup.config.js",
    "build:bundle-docs-hashconnect": "rollup -c ./lib.docs/src/hashconnect/rollup.config.js",
    "build:bundle-docs-hedera-sdk": "rollup -c ./lib.docs/src/sdk/rollup.config.js",
    "build:bundle-docs-strato": "rollup -c ./lib.docs/src/strato/rollup.config.js",
    "build:bundle-test-rollup": "rollup -c ./test/rollup/rollup.config.js",
    "build:declarations": "tsc",
    "build:docs": "run-s build:docs-install-hashconnect build:bundle-docs build:docs-docusaurus",
    "build:docs-docusaurus": "docusaurus build --out-dir=docs lib.docs",
    "build:docs-install-hashconnect": "cd ./lib.docs/src/hashconnect && npm i",
    "clean": "run-s clean:bundle-test-rollup clean:cjs clean:esm clean:declarations && docusaurus clear lib.docs",
    "clean:bundle-test-rollup": "rm -fr test/rollup/lib.esm",
    "clean:cjs": "rm -fr lib.cjs",
    "clean:declarations": "rm -fr types",
    "clean:esm": "rm -fr lib.esm",
    "deploy:docs": "docusaurus deploy",
    "docusaurus-swizzle": "docusaurus swizzle",
    "lint": "eslint . --ext ts",
    "publish:npm": "run-s clean build:4-publish && npm publish --access public",
    "start:docs": "docusaurus start lib.docs",
    "test": "run-s test:node test:jsdom test:rollup",
    "test:ci": "run-s lint test test:rollup",
    "test:browser": "run-s build:bundle-test-rollup test:smoke-rollup",
    "test:node": "jest --env=./test/jte.node.ts",
    "test:node-previewnet": "PORTAL_NETWORK=previewnet node ./script/test-online.mjs",
    "test:node-testnet": "PORTAL_NETWORK=testnet node ./script/test-online.mjs",
    "test:jsdom": "jest --env=./test/jte.web.ts --setupFilesAfterEnv core-js ./test/setup.web.ts",
    "test:smoke-rollup": "karma start ./test/rollup/karma.smoke.conf.js"
  },
  "keywords": [
    "Hedera",
    "Hashgraph",
    "Smart Contracts"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "@ethersproject/abi": "^5.5.0",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "dotenv": "^10.0.0",
    "magic-string": "^0.25.7",
    "rollup": "^2.68.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-web-worker-loader": "^1.6.1",
    "solc": "0.8.9",
    "traverse": "^0.6.6",
    "tslib": "^2.3.1",
    "winston": "^3.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@cmfcmf/docusaurus-search-local": "^0.10.0",
    "@docusaurus/core": "2.0.0-beta.15",
    "@docusaurus/preset-classic": "2.0.0-beta.15",
    "@docusaurus/remark-plugin-npm2yarn": "2.0.0-beta.15",
    "@docusaurus/theme-live-codeblock": "2.0.0-beta.15",
    "@hashgraph/sdk-web": "npm:@hashgraph/sdk@2.11.0-beta.1",
    "@openzeppelin/contracts": "^4.4.2",
    "@peculiar/webcrypto": "^1.3.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@types/estree": "^0.0.51",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "axios": "^0.26.0",
    "core-js": "^3.20.3",
    "docusaurus": "^1.14.7",
    "elliptic": "^6.5.4",
    "eslint": "^8.8.0",
    "events": "^3.3.0",
    "jest": "^27.4.5",
    "karma": "^6.3.13",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^4.0.1",
    "mdx-mermaid": "^1.2.1",
    "mermaid": "^8.11.5",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^13.1.3",
    "remark-gfm": "^2.0.0",
    "remark-numbered-footnote-labels": "^1.1.0",
    "rollup-plugin-import-map": "^2.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.1.2",
    "tweetnacl": "^1.0.3",
    "typescript": "^4.5.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/buidler-labs/hedera-strato-js"
  },
  "types": "./types/index.d.ts",
  "version": "0.7.3",
  "peerDependencies": {
    "@hashgraph/sdk": "2.11.0-beta.1"
  }
}
