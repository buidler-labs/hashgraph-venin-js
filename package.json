{
  "author": "BuiDler Labs Developers <devs@buidlerlabs.com>",
  "description": "The Hedera unoficial, high-level 'stratospheric' SDK for JS consumption",
  "engines": {
    "node": ">=14.8"
  },
  "exports": {
    "require": "./lib.cjs/index.js",
    "import": "./lib.esm/index.js"
  },
  "files": [
    "lib"
  ],
  "main": "./lib.cjs/index.js",
  "module": "./lib.esm/index.js",
  "name": "@buidlerlabs/hedera-strato-js",
  "scripts": {
    "build:cjs": "BABEL_ENV=cjs babel lib --extensions '.ts' --out-dir 'lib.cjs' --source-maps",
    "build:esm": "BABEL_ENV=esm babel lib --extensions '.ts' --out-dir 'lib.esm' --source-maps",
    "build:bundle-docs": "rollup -c ./docs/src/strato/rollup.config.js",
    "build:bundle-test-browser": "rollup -c ./test/browser/rollup.config.js",
    "build:declarations": "tsc",
    "build:docs": "docusaurus build --out-dir=docs.html docs",
    "clean": "rm -fr build dist && npm run clean:bundle-test-browser && docusaurus clear",
    "clean:bundle-test-browser": "rm -fr test/browser/lib.esm",
    "deploy:docs": "docusaurus deploy",
    "docusaurus-swizzle": "docusaurus swizzle",
    "install:test-browser-dependencies": "cd test/browser && npm i",
    "lint": "eslint . --ext ts",
    "publish:npm": "run-s clean build:cjs build:declarations build:esm && npm publish --access public",
    "start:docs": "docusaurus start docs",
    "test": "run-s test:node test:jsdom",
    "test:ci": "run-s lint test test:browser",
    "test:browser": "run-s install:test-browser-dependencies build:bundle-test-browser test:smoke-browser",
    "test:node": "jest --env=./test/jte.node.ts",
    "test:node-previewnet": "PORTAL_NETWORK=previewnet node ./script/test-online.mjs",
    "test:node-testnet": "PORTAL_NETWORK=testnet node ./script/test-online.mjs",
    "test:jsdom": "jest --env=./test/jte.web.ts --setupFilesAfterEnv core-js",
    "test:smoke-browser": "karma start ./test/browser/karma.smoke.conf.js"
  },
  "keywords": [
    "Hedera",
    "Hashgraph",
    "Smart Contracts"
  ],
  "license": "MIT",
  "dependencies": {
    "@ethersproject/abi": "^5.5.0",
    "@hashgraph/sdk": "2.7.0",
    "dotenv": "^10.0.0",
    "solc": "0.8.9",
    "traverse": "^0.6.6",
    "tslib": "^2.3.1",
    "winston": "^3.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@cmfcmf/docusaurus-search-local": "^0.10.0",
    "@docusaurus/core": "^2.0.0-beta.15",
    "@docusaurus/preset-classic": "^2.0.0-beta.15",
    "@docusaurus/remark-plugin-npm2yarn": "^2.0.0-beta.15",
    "@docusaurus/theme-live-codeblock": "^2.0.0-beta.15",
    "@openzeppelin/contracts": "^4.4.2",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "axios": "^0.26.0",
    "core-js": "^3.20.3",
    "docusaurus": "^1.14.7",
    "elliptic": "^6.5.4",
    "eslint": "^8.8.0",
    "events": "^3.3.0",
    "jest": "^27.4.5",
    "karma": "^6.3.13",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^4.0.1",
    "mdx-mermaid": "^1.2.1",
    "mermaid": "^8.11.5",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^13.1.3",
    "remark-gfm": "^2.0.0",
    "remark-numbered-footnote-labels": "^1.1.0",
    "rollup": "^2.67.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-web-worker-loader": "^1.6.1",
    "ts-jest": "^27.1.2",
    "tweetnacl": "^1.0.3",
    "typescript": "^4.5.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/buidler-labs/hedera-strato-js"
  },
  "types": "./types/index.d.ts",
  "version": "0.6.10"
}
