{
  "author": "BuiDler Labs Developers <devs@buidlerlabs.com>",
  "description": "The Hedera unoficial, high-level 'stratospheric' SDK for JS consumption",
  "engines": {
    "node": ">=14.8"
  },
  "exports": {
    "require": "./lib.cjs/index.js",
    "import": "./lib.esm/index.js"
  },
  "files": [
    "lib",
    "output"
  ],
  "main": "./lib.cjs/index.js",
  "module": "./lib.esm/index.js",
  "name": "@buidlerlabs/hedera-strato-js",
  "scripts": {
    "build:cjs": "BABEL_ENV=cjs babel lib --extensions '.ts' --out-dir 'build/lib.cjs' --source-maps",
    "build:esm": "BABEL_ENV=esm babel lib --extensions '.ts' --out-dir 'build/lib.esm' --source-maps",
    "build:bundle-test-browser": "rollup -c ./test/browser/rollup.config.js",
    "build:declarations": "tsc",
    "build:test": "BABEL_ENV=test babel test --extensions '.ts' --out-dir 'build/test'",
    "clean": "rm -fr build dist && npm run clean:bundle-test-browser",
    "clean:bundle-test-browser": "rm -fr test/browser/lib.esm",
    "docs": "typedoc && node ./script/post-docs.cjs",
    "publish:npm": "run-s clean build:declarations && node ./script/prepare-publish.cjs && npm publish ./build --access public",
    "test": "run-s test:node test:jsdom",
    "test:ci": "run-s test test:browser",
    "test:browser": "run-s build:bundle-test-browser test:smoke-browser",
    "test:node": "jest --env=./test/jte.node.ts",
    "test:jsdom": "jest --env=./test/jte.web.ts --setupFilesAfterEnv core-js",
    "test:smoke-browser": "karma start ./test/browser/karma.smoke.conf.js"
  },
  "keywords": [
    "Hedera",
    "Hashgraph",
    "Smart Contracts"
  ],
  "license": "MIT",
  "dependencies": {
    "@ethersproject/abi": "^5.5.0",
    "@hashgraph/sdk": "^2.6.0",
    "axios": "^0.25.0",
    "dotenv": "^10.0.0",
    "solc": "0.8.9",
    "traverse": "^0.6.6",
    "tslib": "^2.3.1",
    "winston": "^3.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@openzeppelin/contracts": "^4.4.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@rollup/plugin-virtual": "^2.0.3",
    "core-js": "^3.20.3",
    "events": "^3.3.0",
    "jest": "^27.4.5",
    "karma": "^6.3.13",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^13.1.3",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.1.2",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/buidler-labs/hedera-strato-js"
  },
  "types": "./types/index.d.ts",
  "version": "0.6.10"
}
